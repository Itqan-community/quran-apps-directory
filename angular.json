{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": "1e1de97b-a744-405a-8b5a-0397bb3d01ce"
  },
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "aot": true,
              "extractLicenses": true,
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true
            },
            "develop": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.develop.ts"
                }
              ],
              "aot": true,
              "extractLicenses": true,
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "aot": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true,
                "fonts": {
                  "inline": true
                }
              },
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": false,
                "hidden": true
              },
              "crossOrigin": "anonymous",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5mb",
                  "maximumError": "2.0mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "40kb"
                }
              ]
            }
          },
          "options": {
            "prerender": false,
            "ssr": false,
            "assets": [
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/robots.txt",
              "src/manifest.webmanifest",
              "src/sw.js",
              {
                "glob": "_redirects",
                "input": "src/assets",
                "output": "/"
              },
              {
                "glob": "_headers",
                "input": "src/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "index": "src/index.html",
            "browser": "src/main.ts",
            "outputPath": "dist",
            "polyfills": ["zone.js"],
            "scripts": [],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.css",
              "src/global_styles.css",
              "./src/themes.scss"
            ],
            "tsConfig": "tsconfig.app.json",
            "serviceWorker": "src/ngsw-config.json",
            "optimization": {
              "fonts": true,
              "styles": {
                "inlineCritical": false,
                "minify": true
              },
              "scripts": true
            }
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "demo:build:development"
            },
            "production": {
              "buildTarget": "demo:build:production"
            }
          },
          "defaultConfiguration": "development"
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {},
      "sourceRoot": "src"
    }
  }
}
