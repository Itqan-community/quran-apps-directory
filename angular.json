  "version": 1,
  "cli": {
    "analytics": "1e1de97b-a744-405a-8b5a-0397bb3d01ce"
  },
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true
            },
            "staging": {
              "aot": true,
              "extractLicenses": true,
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true
            },
            "production": {
              "aot": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": {
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "scripts": true,
                "fonts": {
                  "inline": true
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "crossOrigin": "anonymous",
              "serviceWorker": "ngsw-config.json",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "40kb"
                }
              ]
            }
          },
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/robots.txt",
              "src/sw.js",
              "src/assets"
            ],
            "index": "src/index.html",
            "browser": "src/main.ts",
            "outputPath": "dist",
            "polyfills": [
              "zone.js"
            ],
            "scripts": [],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.css",
              "src/global_styles.css",
              "./src/themes.scss"
            ],
            "tsConfig": "tsconfig.app.json",
            "server": "src/main.server.ts",
            "ssr": {
              "entry": "src/server.ts"
            }
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "demo:build:development"
            },
            "production": {
              "buildTarget": "demo:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
