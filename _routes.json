{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/assets/*",
    "/images/*",
    "/*.js",
    "/*.css",
    "/*.ico",
    "/*.svg",
    "/sitemap.xml",
    "/robots.txt"
  ],
  "rules": [
    {
      "type": "rewrite",
      "pattern": "^(?!/api).*$",
      "destination": "/index.html"
    }
  ]
}
